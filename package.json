{
  "scripts": {
    "docker:build": "docker build --cpuset-cpus 2 -t hsldevcom/hsl-map-reports .",
    "docker:run": "docker run --name map-reports -p 1234:1234 hsldevcom/hsl-map-reports",
    "docker:postgis": "docker run --name map-reports-postgis -e POSTGRES_PASSWORD=mysecretpassword -d mdillon/postgis",
    "watch": "parcel watch index.html --public-url ./dist/",
    "build": "parcel build index.html --public-url ./dist/",
    "server": "ts-node --project tsconfig.server.json server/server.ts",
    "server:dev": "nodemon --delay 1 --signal SIGTERM --watch 'server/**/*' --exec 'ts-node --project tsconfig.server.json' server/server.ts",
    "knex": "PG_CONNECTION_STRING=postgres://postgres:mysecretpassword@localhost:5432/postgres knex",
    "start": "concurrently 'yarn run server:dev' 'yarn run watch'",
    "production": "forever start -c \"ts-node --project tsconfig.server.json\" server/server.ts -l server.log -o server-out.log -e server-err.log && forever -f logs 0",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
